
<div id="tag">
  <h1><%=page.tag%></h1>
  <% page.posts.each(article => { %>
    <div class="tag-item">
      <a href="<%=url_for(article.path)%>">
        <%=article.title%>
      </a>
      <time datetime="<%=date_xml(article.date)%>">
        <%=date(article.date)%>
      </time>
    </div>
  <% }) %>
</div>

<div id="paginator">
  <%-paginator({prev_text: '上一页',next_text: '下一页'})%>
</div>
